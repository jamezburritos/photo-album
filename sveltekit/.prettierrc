{
	"arrowParens": "always",
	"bracketSameLine": false,
	"bracketSpacing": true,
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"printWidth": 80,
	"semi": false,
	"singleAttributePerLine": true,
	"singleQuote": true,
	"trailingComma": "none",
	"useTabs": true
}